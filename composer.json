{
    "name": "lukaszaleckas/laravel-concurrency",
    "autoload": {
        "psr-4": {
            "LaravelConcurrency\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelConcurrency\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "authors": [
        {
            "name": "lukaszaleckas",
            "email": "luk.zaleckas@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^10.0",
        "ext-pcntl": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.6",
        "orchestra/testbench": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelConcurrency\\ConcurrencyServiceProvider"
            ]
        }
    }
}
